<Window x:Class="NormalMapMaker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NormalMapMaker"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="525">
    <Grid>
        <StackPanel>
            <Image x:Name="LoadedImage" Height="200"></Image>
            <Button Click="OpenClick">Load Image</Button>
            <Image x:Name="SavedImage" Height="200"></Image>
            <Button Click="SaveClick">Save Image</Button>
        </StackPanel>
    </Grid>
</Window>
